<%- include ('partial/header.ejs') %>

    <div class="container py-5">
        <img class="profile" src="./images/jatin.jpg">
        <span>Jatin Rathore</span>
        <span class="float-right">
            <a href="https://www.instagram.com/_._._jatin_._._/?hl=en"><i data-feather="instagram"></i> Instagram</a>
        </span>
    </div>

    <div class="row images" id="gallery" data-toggle="modal" data-target="#exampleModal">
        <div class="col-6 col-sm-4 col-md-3 col-lg-2">
            <img src="./images/jatin/jatin (1).jpg" alt="" data-target=”#carouselExample” data-slide-to="0">
        </div>
        <div class="col-6 col-sm-4 col-md-3 col-lg-2">
            <img src="./images/jatin/jatin (2).jpg" alt="" data-target=”#carouselExample” data-slide-to="1">
        </div>
        <div class="col-6 col-sm-4 col-md-3 col-lg-2">
            <img src="./images/jatin/jatin (3).jpg" alt="" data-target=”#carouselExample” data-slide-to="2">
        </div>
        <div class="col-6 col-sm-4 col-md-3 col-lg-2">
            <img src="./images/jatin/jatin (4).jpg" alt="" data-target=”#carouselExample” data-slide-to="3">
        </div>
        <div class="col-6 col-sm-4 col-md-3 col-lg-2">
            <img src="./images/jatin/jatin (5).jpg" alt="" data-target=”#carouselExample” data-slide-to="4">
        </div>
        <div class="col-6 col-sm-4 col-md-3 col-lg-2">
            <img src="./images/jatin/jatin (6).jpg" alt="" data-target=”#carouselExample” data-slide-to="5">
        </div>
    </div>
    <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <!-- Carousel markup goes in the modal body -->

                    <div id="carouselExample" class="carousel slide" data-ride="carousel">
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <img class="d-block w-100" src="./images/jatin/jatin (1).jpg">
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="./images/jatin/jatin (2).jpg">
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="./images/jatin/jatin (3).jpg">
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="./images/jatin/jatin (4).jpg">
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="./images/jatin/jatin (5).jpg">
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="./images/jatin/jatin (6).jpg">
                            </div>
                        </div>
                        <a class="carousel-control-prev" href="#carouselExample" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExample" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>
                    </div>
                </div>
            </div>
        </div>


        <%- include ('partial/footer.ejs') %>